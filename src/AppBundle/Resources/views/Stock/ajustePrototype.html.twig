<tr class="item" divdata="item_items">
    <td class="ordTd"></td>
    <td class="prodTd">{{ form_widget(item.insumo) }} </td>
    <td class="signoTd">{{ form_widget(item.signo, { 'attr': {'class' : 'selectsigno'} }) }}</td>
    <td class="cantTd">{{ form_widget(item.cantidad, { 'attr': {'class' : 'number floatleft','min':'0'} }) }} </td>        
    <td class="motTd">{{ form_widget(item.motivo, { 'attr': {'rows' : '1'} }) }}</td>
    <td class="delTd" style="text-align:center;"></td>
</tr>